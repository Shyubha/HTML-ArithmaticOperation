<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title></title>
</head>
<body>

    <div id="d1"></div>
    <script>


        // array of numbers
        var numbers = [45, 55, 65, 77, 88, 123];
        numbers.push(555);
        numbers.pop(); // will remove the very last element added
        numbers.sort();  // not sure ..ask to teacher
        numbers.reverse();
        numbers.slice(); // not sure..ask to teacher

        var data = "<ol>";
        for (let i = 0; i < numbers.length; i++) {
            data += "<li>" + numbers[i] + "</li>"
        }
        data += "</ol>";
        document.getElementById("d1").innerHTML = data;

    </script>

</body>
</html>
